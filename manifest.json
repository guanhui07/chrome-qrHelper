{
	"manifest_version": 2,
	"name": "二维码小助手",
	"version": "0.1.2",
	"description": "二维码小助手，根据当前页面URL生成二维码；解析页面上某个二维码",
	"icons": {
		"48": "images/icon_32.png",
		"128": "images/logo.png"
	},
	"browser_action": {
		"default_icon": "images/icon_32.png",
		"default_popup": "popup.html"
	},
	"permissions": [ "tabs", "contextMenus", "*://*/*" ],
	"background": {
		"scripts": ["js/config.js", "js/background.js"]
	},
	"options_page": "options.html"
}